<IfModule mod_rewrite.c>
    RewriteEngine On

    # Make sure .htaccess itself and existing files/folders still work
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d

    # Redirect everything to index.php with url param
    RewriteRule ^(.*)$ index.php?url=$1 [QSA,L]
</IfModule>
